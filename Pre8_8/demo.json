    "description": "Tài liệu API tự động sinh bằng Flask-RESTX"
  },
  "produces": [
    "application/json"
  ],
  "consumes": [
    "application/json"
  ],
  "securityDefinitions": {
    "jwt": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization",
      "description": "Type in the *'Value'* input field: **Bearer \u003CJWT\u003E**, where JWT is the access token"
    }
  },
  "security": [
    {
      "jwt": []
    }
  ],
  "tags": [
